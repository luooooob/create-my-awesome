{
  "name": "create-my-awesome",
  "version": "1.0.0",
  "private": true,
  "description": "Github Actions for automatically generating the personal awesome list from all of the repositories you starred.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build --source-map --license licenses.txt",
    "test:unit": "jest",
    "test:local": "ts-node src/main.ts",
    "all": "npm run build && npm run package && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luob/create-my-awesome.git"
  },
  "keywords": [
    "actions",
    "node",
    "awesome"
  ],
  "author": "luob",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "dotenv": "^8.2.0",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.9",
    "@vercel/ncc": "^0.25.1",
    "jest": "^24.9.0",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}
